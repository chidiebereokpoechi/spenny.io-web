{
    "extends": ["react-app", "react-app/jest"],
    "plugins": [],
    "rules": {
        "indent": "off",
        "linebreak-style": ["error", "unix"],
        "quotes": [
            2,
            "single",
            {
                "avoidEscape": true
            }
        ],
        "semi": ["error", "never"],
        "no-unused-vars": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "@typescript-eslint/no-unused-vars": ["error"],
        "@typescript-eslint/no-non-null-assertion": "off",
        "eqeqeq": "error"
    }
}
